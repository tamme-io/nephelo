{
  "{VPC_NAME}": {
		"Type": "AWS::EC2::VPC",
		"Properties": {
			"CidrBlock": "{CIDR_BLOCK}",
			"InstanceTenancy": "default",
			"EnableDnsSupport": "true",
			"EnableDnsHostnames": "true",
			"Tags": [
				{
					"Key": "Name",
					"Value": "{NAME}"
				},
				{
					"Key": "Stage",
					"Value": "{STAGE}"
				}
			]
		}
	}
}